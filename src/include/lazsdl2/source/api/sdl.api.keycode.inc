  (**
   * \brief Enumeration of valid key mods (possibly OR'd together).
   *)
  {$PackEnum 2}
  SDL_Keymod = (
      KMOD_NONE = $0000,
      KMOD_LSHIFT = $0001,
      KMOD_RSHIFT = $0002,
      KMOD_LCTRL = $0040,
      KMOD_RCTRL = $0080,
      KMOD_LALT = $0100,
      KMOD_RALT = $0200,
      KMOD_LGUI = $0400,
      KMOD_RGUI = $0800,
      KMOD_NUM = $1000,
      KMOD_CAPS = $2000,
      KMOD_MODE = $4000,
      KMOD_RESERVED = $8000
  );
  {$PackEnum 4}

  SDL_Keymods = record Value: SDL_UInt32; end;

